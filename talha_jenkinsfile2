pipeline{
   agent any
   tools {
        maven 'mvn-3.6.0'
        jdk 'myJDK'
   }
   stages {
      stage('Pulling') { 
         steps{
           git 'https://github.com/talhakhannnnn/jenkins-practice-repo.git'
         }
       }
      stage ('Compile Stage'){
         steps{
               bat "mvn -Dmaven.test.failure.ignore clean install"
         }
      }
   }
}
