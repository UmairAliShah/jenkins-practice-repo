node {
   def mvnHome
   def javaHome
   stage('Preparation') { 
      git 'https://github.com/talhakhannnnn/jenkins-practice-repo.git'
      
      mvnHome = tool 'mvn-3.6.0'
      javaHome = tool 'myJDK'
   }
   stage('Build') {
      echo 'Building'
      bat(/"${javaHome}\bin\java" -version/)
      bat(/"${mvnHome}\bin\mvn" -version/)
   }
   
}
